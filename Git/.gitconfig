[user]
	email = gpanayiotou@server.fake
	name = Georgios Panayiotou
[color]
  ui = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
  whitespace = red reverse
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[core]
  whitespace=fix,-indent-with-non-tab,trailing-space,cr-at-eol
[credential]
[credential]
[credential]
[core]
	autocrlf = true
	excludesfile = ~/.gitignore_global
[credential]
	helper = wincred
